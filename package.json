{
  "name": "react-stripe-store",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "concurrently --kill-others \"next dev\" \"npm run listen\"",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "format": "npx prettier --write .",
    "clean": "npm run lint && npm run format",
    "prepare": "husky install",
    "listen": "stripe listen --forward-to localhost:3000/webhook"
  },
  "dependencies": {
    "@emotion/react": "^11.10.6",
    "@emotion/styled": "^11.10.6",
    "@mui/material": "^5.12.0",
    "@types/node": "18.15.11",
    "@types/react": "18.0.34",
    "@types/react-dom": "18.0.11",
    "eslint": "8.38.0",
    "eslint-config-next": "13.3.0",
    "next": "13.3.0",
    "pusher": "^5.1.2",
    "pusher-js": "^8.0.2",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "rxjs": "^7.8.0",
    "stripe": "^12.0.0",
    "typescript": "5.0.4"
  },
  "devDependencies": {
    "@svgr/webpack": "^7.0.0",
    "@trivago/prettier-plugin-sort-imports": "^4.1.1",
    "@typescript-eslint/eslint-plugin": "^5.58.0",
    "concurrently": "^8.0.1",
    "eslint-config-prettier": "^8.8.0",
    "husky": "^8.0.3",
    "prettier": "2.8.7",
    "sass": "^1.62.0"
  }
}
